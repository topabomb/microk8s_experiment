  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev -m spa",
    "build": "quasar build -m spa",
    "build:image:dev": "npm run build && docker build -t 172.18.72.18:32000/website-app ./",
    "pull:image:dev": "npm run build:image:dev && docker pull 172.18.72.18:32000/website-app",
    "deploy:dev": "npm run pull:image:dev && kubectl apply -n default -f ./deploy/deployment.yaml"
  },
  "dependencies": {
